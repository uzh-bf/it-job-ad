{
  "name": "uzh-bf-joinus",
  "description": "Overview of IT Projects and Positions at the Dept. of Banking and Finance, University of Zurich",
  "homepage": "https://uzh-bf.github.io/joinus/",
  "version": "2.0.0",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/uzh-bf/joinus"
  },
  "maintainers": [
    "Manuel Keller <manuel.keller@bf.uzh.ch>",
    "Roland Schlaefli <roland.schlaefli@bf.uzh.ch>"
  ],
  "scripts": {
    "clean": "rimraf node_modules/.cache out/ .next/",
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build",
    "export": "cross-env NODE_ENV=production next export",
    "build:semantic": "cd semantic && gulp build-css build-assets",
    "start": "next start",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "npm run clean && npm run build:semantic && npm run build && npm run export && touch out/.nojekyll && gh-pages -d out -t true"
  },
  "dependencies": {
    "animated-scroll-to": "2.0.12",
    "fomantic-ui": "2.8.7",
    "next": "9.5.5",
    "react": "16.14.0",
    "react-burger-menu": "2.9.0",
    "react-dom": "16.14.0",
    "semantic-ui-react": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@storybook/addon-actions": "6.1.20",
    "@storybook/addon-essentials": "6.1.15",
    "@storybook/addon-links": "6.1.20",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "6.1.20",
    "@types/react": "17.0.2",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "9.1.2",
    "cross-env": "7.0.3",
    "prettier": "2.2.1",
    "react-is": "16.13.1",
    "rimraf": "3.0.2",
    "sass": "1.32.8",
    "typescript": "4.1.3"
  }
}
